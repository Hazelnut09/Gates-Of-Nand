/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
Not(in=sel, out=c);
And(a=a, b=c, out=e);
And(a=sel, b=b, out=f);
Or(a=e, b=f, out=out);
}